# MinIO credentials
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_WEBHOOK_TOKEN=webhook-secret

# Public domain and ports for external access
PUBLIC_DOMAIN=localhost
PUBLIC_WEB_PORT=80
PUBLIC_MINIO_PORT=81
PUBLIC_MINIO_USE_SSL=false

# Internal service endpoints (for Docker services to communicate)
INTERNAL_MINIO_ENDPOINT=minio
INTERNAL_MINIO_PORT=9000

# Direct MinIO access (for development/debugging)
MINIO_API_PORT=9090
MINIO_CONSOLE_PORT=9091

# Redis Configuration
REDIS_PASSWORD=

# MongoDB Configuration
MONGO_ROOT_USER=admin
MONGO_ROOT_PASSWORD=changeme

# Ingestion Service Configuration
OLLAMA_TEXT_MODEL=nomic-embed-text
OLLAMA_IMAGE_MODEL=nomic-embed-vision
MAX_RETRIES=3
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
LOG_LEVEL=debug